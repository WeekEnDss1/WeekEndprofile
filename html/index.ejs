const Toast = Swal.mixin({
  toast: true,
  position: 'top-end',
  showConfirmButton: false,
  timer: 3000,
  background:'#212121',
  timerProgressBar: true,
  didOpen: (toast) => {
    toast.addEventListener('mouseenter', Swal.stopTimer)
    toast.addEventListener('mouseleave', Swal.resumeTimer)
  }
})


async function submitFeedBack() {
    const message = document.getElementById('feedback').value
    console.log(message)
    if(!message || message <0) return Toast.fire({
                            icon: 'error',
                            background:'#212121',
                            title: 'Feedback input must not be empty !'
                          })
                          Toast.fire({
                            icon: 'warning',
                            background:'#212121',
                            title: 'Please wait while request api !'
                          })
    const ApiRequest = await fetch('/api/v1/users/feedbacks',{
        method:'POST',

        headers:{
            'Content-Type':'application/json',
            'user-auth':'google-auth-seassion'
        },
        body:JSON.stringify({
            message:message
        })

    })
    const ApiResult = await ApiRequest.json()
     
    if(ApiResult.action === 'success'){
        Toast.fire({
                            icon: 'success',
                            background:'#212121',
                            title: ApiResult.message
                          })
                        window.location.href='#close-modal'
    }else {
        Toast.fire({
                            icon: 'error',
                            background:'#212121',
                            title: message 
                          })
    }
    
}


